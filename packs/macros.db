    roll.total == actor.data.data.unlucky_numbers.ul2 || \n                roll.total == actor.data.data.unlucky_numbers.ul3 || \n                roll.total == actor.data.data.unlucky_numbers.ul4 || \n                roll.total == actor.data.data.unlucky_numbers.ul5 ||\n                roll.total == actor.data.data.unlucky_numbers.ul6)\n\n{\n            const content = `Rolls for <b>Physical</b>!\n            <p></p><b>Target Number: [[${actor.data.data.professions.physical}]]</b> <p></p>\n            <b>Result: [[${roll.total}]]</b><p></p>\n            <span style='color:red; font-size:120%;'> <b>UNLUCKY NUMBER!</b></span>`\n        \n            roll.toMessage({type: 4, user: game.user._id, speaker: ChatMessage.getSpeaker(), content: content});\n        \n          } else {\n            const content = `Rolls for <b>Physical</b>!\n            <p></p><b>Target Number: [[${actor.data.data.professions.physical} + ${playerInput}]]</b> <p></p>\n            <b>Result: [[${roll.total}]]</b><p></p>\n            ${roll.total<=(actor.data.data.professions.physical + playerInput) ? \" <span style='color:green; font-size: 120%;'> <b>SUCCESS!</b></span>\" : \" <span style='color:red; font-size: 120%;'> <b>FAILURE!</b></span>\"}`\n        \n            roll.toMessage({type: 4, user: game.user._id, speaker: ChatMessage.getSpeaker(), content: content});\n            }\n        }\n      },\n      two: {\n        label: \"Cancel\",\n        callback: html => console.log(\"Cancelled\")\n      }\n    },\n    default: \"one\",\n    close: html => console.log()\n  });\n  d.render(true);","author":"azmvrHPGsACq7icE","img":"icons/equipment/hand/gauntlet-tooled-leather-brown.webp","actorIds":[]}
